                "windows": [
                    "venv\\Scripts\\python test.py"
                ]
            }
        ],
        "update": [
            {
                "linux": [
                    "venv/bin/pip freeze > requirements.txt",
                    "venv/bin/pip install --upgrade pip"
                ],
                "windows": [
                    "venv\\Scripts\\pip freeze > requirements.txt",
